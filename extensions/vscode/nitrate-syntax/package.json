{
    "name": "nitrate-syntax",
    "displayName": "Nitrate: Syntax Highlighting",
    "publisher": "nitrate",
    "description": "Nitrate syntax highlighting support",
    "icon": "./icons/icon-light.png",
    "version": "3.6.0",
    "repository": "https://github.com/Kracken256/nitrate/extensions/vscode/nitrate-syntax",
    "engines": {
        "vscode": "^1.92.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "nitrate",
                "aliases": [
                    "Nitrate",
                    "NITRATE",
                    "nitrate"
                ],
                "extensions": [
                    ".nit"
                ],
                "icon": {
                    "light": "./icons/icon-light.png",
                    "dark": "./icons/icon-dark.png"
                },
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "nitrate",
                "scopeName": "source.nit",
                "path": "./nitrate.tmLanguage.json"
            }
        ]
    }
}